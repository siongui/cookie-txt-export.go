{
  "manifest_version": 2,

  "name": "cookie-txt-export.go",
  "description": "Exports cookie data in a format that can be used by wget",
  "version": "0.1",

  "browser_action": {
    "default_title": "export cookie.txt",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
